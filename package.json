      "@semantic-release/github"
    ]
  },
  "bugs": {
    "url": "https://github.com/Porok12/epic-jira/issues"
  },
  "devDependencies": {
    "semantic-release": "^22.0.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  }
