{
  "name": "epic-jira",
  "version": "0.1.0",
  "homepage": "https://github.com/Porok12/epic-jira",
  "description": "Dashboards for Jira",
  "author": "Przemys≈Çaw Papla",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "jira"
  ],
  "main": "dist/app.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Porok12/epic-jira.git"
  },
  "scripts": {
    "start": "tsc && node dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "release": {
    "branches": [
      "master",
